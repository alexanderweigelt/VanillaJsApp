{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.10.0",
    "storageDir": ".openapi/storage/dir",
    "generators": {
      "clients": {
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/packages/clients/#{name}",
        "glob": ".openapi/specification/*.{json,yaml}",
        "packageName": "#{name}",
        "additionalProperties": {
          "supportsES6": true,
          "withInterfaces": true,
          "npmName": "@my-client/#{name}"
        }
      }
    }
  }
}
